clc;
clear;
close all;
method = input('Enter the number (one to five): ','s');
 
switch lower(method)
  case 'one'
    disp('Method is One')
  case 'two'
    disp('Method is two')
  case 'three'
    disp('Method is three')
  case four
    disp('Method is four')
  case five
    disp('Method is five')
  otherwise
    disp('Unknown method.')
end

